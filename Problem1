import java.util.Scanner;
import java.util.Random;
public class ComputeMethods5026201026{

	public static void main(String[] args){
		System.out.println("Nomor1, Konversi suhu dari Fahrenheit ke Celsius");
		double Fahrenheit;
		Scanner inputUser = new Scanner(System.in);
		System.out.println("Masukan suhu dalam Fahrenheit :");
		Fahrenheit = inputUser.nextInt();
		Fahrenheit = ((Fahrenheit - 32)*5)/9;
		System.out.println("Suhu dalam celsius : "+Fahrenheit);

		System.out.println("\n");
		
		Scanner angka = new Scanner(System.in); 
		int a,b,c;
		System.out.println("Nomor2, Formula pythagoras menggunakan segitiga siku - siku");
		System.out.println("Dimana diketahui a adalah sisi tegak, b adalah sisi samping, dan c adalah sisi miring");		
		System.out.println("Masukan nilai a :");
		a = angka.nextInt();
		System.out.println("Masukan nilai b :");
		b = angka.nextInt();
		c = ((a*a)+(b*b));
		System.out.println("Nilai C ="+(Math.sqrt(c)));

		System.out.println("\n");
		System.out.println("Nomor3, Jumlah dua sisi dadu yang dilemparkan");
		Random num = new Random();
		int randomnum = num.nextInt(12) + 2;
		System.out.println("Jumlah dua sisi dadu: "+randomnum);
			
	}
}
